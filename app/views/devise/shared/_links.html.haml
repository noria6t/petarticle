- if controller_name != 'sessions'
  .account-register-btn-link
    = link_to "ログイン画面へ", new_session_path(resource_name), class: 'account-register-btn'
    %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  .account-register-btn-link
    = link_to "新規登録画面へ", new_registration_path(resource_name), class: 'account-register-btn'
    %br/
