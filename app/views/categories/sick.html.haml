.header-select-btn
  = render 'articles/header-btn' 

.main-index-box.displayflex
  .main-contents.fadein
    .index-render-container
      .index-render    
        .index-render-box-category-walk.index-render-box-lightgreen
          %p4 #{@categories[2].name}
          = render partial: 'articles/article', collection:  @articles_sick
      .kaminari-category
        = paginate( @articles_sick)          
  .side-bar
    = render 'shared/side-bar'         